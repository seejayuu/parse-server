	<section id="shared" class="section" style="background-color:#FFF; border-top: 1px solid rgba(0, 0, 0, .1);">
	<div class="container">
			<div class="row">
				<div class="col-md-12">
				  <form class="form-horizontal" role="form" method="post" id="password_form" action="/pwresetdone/<%=key%>"> 
    				<div class="modal-header">
    					<h4>Reset Password<h4>
	    			</div>
	    			<div class="modal-body">
    					<div class="form-group">
    						<label for="password" class="col-sm-2 control-label">New password</label>
    						<div class="col-sm-10">
    							<input type="password" class="form-control" name="password" id="password" placeholder="At least 8 characters">
    						</div>
    					</div>
    					<div class="form-group">
    						<label for="password-again" class="col-sm-2 control-label">Password again</label>
    						<div class="col-sm-10">
    							<input type="password" class="form-control" id="password-again" placeholder="">
    						</div>
    					</div>
	    			</div>
	    			<div class="modal-footer">
    					<button onclick=" if (validatePassword(document.getElementById('password').value, document.getElementById('password-again').value)) document.getElementById('password_form').submit();" type="submit" class="btn btn-primary">Reset Password</button>    				
					</div>
    			</form>
				</div>
			</div>
		</div>
	</section>
